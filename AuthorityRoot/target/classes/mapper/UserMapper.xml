<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zinc.authoritydemo1.mapper.UserMapper">
    <!--查询全部以及通过用户名字查询-->
    <select id="findAll" resultType="com.zinc.authoritydemo1.entity.UserEntity">
    SELECT * FROM authorityuser
    </select>

    <select id="findByName"  parameterType="string" resultType="com.zinc.authoritydemo1.entity.PermissionEntity">
    SELECT #{name} FROM authorityuser
    </select>

    <delete id="deleteByName" parameterType="string">
    DELETE FROM authoriyuser WHERE USERNAME = #{0}
    </delete>
